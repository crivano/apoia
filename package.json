        },
        "moduleFileExtensions": [
            "js",
            "ts",
            "json",
            "txt"
        ],
        "testMatch": [
            "**/__tests__/**/*.+(test.ts|spec.ts)",
            "**/?(*.)+(spec|test).+(ts|tsx)"
